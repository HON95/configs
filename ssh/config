# Add all SSH keys to use (ordered), plus default username if different from local machine
Host *
    User hon
    IdentityFile ~/.ssh/id_rsa_something
    IdentityFile ~/.ssh/id_rsa

# Set user and identity file for git@github.com
Host github.com
    User git
    IdentityFile ~/.ssh/id_rsa_something

